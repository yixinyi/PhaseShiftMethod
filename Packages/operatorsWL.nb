   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t", 
    RowBox[{
     SubscriptBox["\[Psi]", "r"], "=", " ", 
     RowBox[{
      SuperscriptBox["g", 
       RowBox[{"1", "/", "2"}]], "\[Psi]"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t", 
    RowBox[{
     RowBox[{"D", "'"}], "=", 
     RowBox[{
      SuperscriptBox["g", 
       RowBox[{"1", "/", "2"}]], "D", "  ", 
      SuperscriptBox["g", 
       RowBox[{
        RowBox[{"-", "1"}], "/", "2"}]]}]}]}], TraditionalForm]}]]
}], "Text",
