     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
      RowBox[{
       RowBox[{"\[DifferentialD]", "p"}], 
       FractionBox[
        RowBox[{
         RowBox[{"\[Delta]", "'"}], 
         RowBox[{"(", "p", ")"}]}], "\[Pi]"], "log", " ", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["\[Omega]", "2"], "+", 
         SuperscriptBox["p", "2"]}], ")"}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"    ", 
    RowBox[{"=", 
     RowBox[{
      SubsuperscriptBox[
       RowBox[{"[", 
        RowBox[{
         FractionBox[
          RowBox[{"\[Delta]", "(", "p", ")"}], "\[Pi]"], "log", " ", 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["\[Omega]", "2"], "+", 
           SuperscriptBox["p", "2"]}], ")"}]}], "]"}], "0", "\[Infinity]"], 
