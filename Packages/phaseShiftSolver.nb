Cell[TextData[{
 "We have then:\n\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"-", "A"}], " ", 
      RowBox[{"Sin", "[", "\[Delta]", "]"}]}], "=", 
