Cell[TextData[{
 "We want to fit ",
 "\[Phi](x)",
 " with:\n\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    SuperscriptBox["x", "\[Nu]"], "(", 
    RowBox[{
     RowBox[{"a", " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[ImaginaryI]", " ", "p", " ", "x"}]]}], "+", 
     RowBox[{"b", " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", "p", " ", "x"}]]}]}], ")"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t", 
    RowBox[{"\[Nu]", " ", "\[Element]", "Real"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t", 
    RowBox[{"a", ",", " ", 
     RowBox[{"b", " ", "\[Element]", " ", "Complex"}]}]}], TraditionalForm]}]],
 "\nwhich in terms of cosine and sine are:"
}], "Text",
 CellChangeTimes->{{3.690491819336512*^9, 3.690491967819396*^9}, {
  3.690492115498414*^9, 3.690492149719035*^9}, {3.690492332506068*^9, 
  3.690492332851851*^9}, {3.6904983772445183`*^9, 3.6904984077674913`*^9}, {
  3.6904984834761257`*^9, 3.690498504389358*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"a", ",", "b", ",", "p"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"a", " ", 
       RowBox[{"Exp", "[", 
        RowBox[{"\[ImaginaryI]", " ", "p", " ", "x"}], "]"}]}], "+", 
      RowBox[{"b", " ", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "\[ImaginaryI]"}], " ", "p", " ", "x"}], "]"}]}]}], "//",
      "ExpToTrig"}], ",", 
    RowBox[{
     RowBox[{"A", " ", 
      RowBox[{"Cos", "[", 
       RowBox[{
        RowBox[{"p", " ", "x"}], "+", " ", "\[Delta]"}], "]"}]}], "//", 
     "TrigExpand"}]}], "}"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Collect", "[", 
    RowBox[{"#", ",", 
     RowBox[{"{", 
