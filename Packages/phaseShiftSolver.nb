  3.693319013486595*^9, 3.693319104095501*^9}}],

Cell[TextData[{
 "In terms of the momentum:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"\[DifferentialD]", "p"}], " ", "R"}], "+", 
     RowBox[{
      RowBox[{"\[Delta]", "'"}], 
      RowBox[{"(", "p", ")"}], 
      RowBox[{"\[DifferentialD]", "p"}]}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Pi]", " ", 
       RowBox[{"\[DifferentialD]", "n"}]}], "\[Rule]", " ", 
      FractionBox[
       RowBox[{"\[DifferentialD]", "n"}], 
       RowBox[{"\[DifferentialD]", "p"}]]}], "=", 
     RowBox[{
      FractionBox["R", "\[Pi]"], "+", 
      FractionBox[
       RowBox[{
        RowBox[{"\[Delta]", "'"}], 
        RowBox[{"(", "p", ")"}]}], "\[Pi]"]}]}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.634973252521791*^9, 3.634973328314063*^9}, {
  3.636960614891667*^9, 3.636960616923555*^9}, {3.638682318532967*^9, 
  3.638682325580698*^9}, {3.65121242768626*^9, 3.6512124328054733`*^9}, {
  3.6513833594936447`*^9, 3.651383369437714*^9}, {3.6933190259245043`*^9, 
  3.693319029565943*^9}, {3.6933191135631323`*^9, 3.69331911393892*^9}}],

Cell[TextData[{
 "\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{"D", "=", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
     RowBox[{
      RowBox[{"\[DifferentialD]", 
       RowBox[{"p", "(", " ", 
        RowBox[{
         FractionBox[
          RowBox[{"\[DifferentialD]", "n"}], 
          RowBox[{"\[DifferentialD]", "p"}]], " ", "-", 
         RowBox[{
          FractionBox[
           RowBox[{"\[DifferentialD]", "n"}], 
           RowBox[{"\[DifferentialD]", "p"}]], "[", 
          RowBox[{
           RowBox[{"\[Delta]", "(", "p", ")"}], "=", "0"}], "]"}]}], ")"}]}], 
      "log", " ", 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["\[Omega]", "2"], "+", 
        SuperscriptBox["p", "2"]}], ")"}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"    ", 
