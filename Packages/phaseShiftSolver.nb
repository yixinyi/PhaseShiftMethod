   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"a", "+", "b"}], ")"}], " ", 
     RowBox[{"Cos", "[", 
      RowBox[{"p", " ", "x"}], "]"}]}], "+", 
