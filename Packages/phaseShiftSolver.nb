Cell[TextData[{
 "D is obviously invariant under a constant shift: ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", " ", "\[Rule]", " ", 
    RowBox[{"\[Delta]", " ", "+", " ", "const"}]}], TraditionalForm]]],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"D", "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox[
       RowBox[{"[", 
        RowBox[{
         FractionBox[
          RowBox[{
           RowBox[{"\[Delta]", "(", "p", ")"}], "+", "const"}], "\[Pi]"], 
         "log", " ", 
         RowBox[{"(", 
