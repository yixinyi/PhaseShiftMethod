  3.693318947748383*^9, 3.693318966404612*^9}}]
}, Closed]],

Cell[CellGroupData[{

Cell["Determinant before \[Omega] integration", "Subsubsection",
 CellChangeTimes->{{3.636960570075116*^9, 3.63696057426422*^9}, {
  3.651026278214974*^9, 3.651026288147025*^9}}],

Cell[TextData[Cell[BoxData[{
 FormBox[
  RowBox[{"A", "=", 
   RowBox[{
    RowBox[{"tr", " ", "log", " ", 
     FractionBox["O", 
      SubscriptBox["O", "reg"]]}], "=", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
     RowBox[{
      FractionBox[
       RowBox[{"\[DifferentialD]", "\[Omega]"}], 
       RowBox[{"2", "\[Pi]"}]], "D"}]}]}]}], 
  TraditionalForm], "\[IndentingNewLine]", 
 FormBox[
  RowBox[{"\t", 
   RowBox[{"D", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Sum]", "n"], 
      RowBox[{"log", " ", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["\[Omega]", "2"], "+", 
         SuperscriptBox[
          SubscriptBox["p", "n"], "2"]}], ")"}]}]}], "-", 
     RowBox[{
      SubscriptBox["\[Sum]", "n"], 
      RowBox[{"log", " ", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["\[Omega]", "2"], "+", 
         SuperscriptBox[
          SubscriptBox["p", 
           RowBox[{"0", "n"}]], "2"]}], ")"}]}]}]}]}]}], 
  TraditionalForm]}]]], "Text",
 CellChangeTimes->{{3.634972927565275*^9, 3.634972995784094*^9}, {
  3.6349731095845537`*^9, 3.634973112368245*^9}, {3.634973552376796*^9, 
