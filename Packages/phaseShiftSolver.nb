Cell["2nd order ODE: Schr\[ODoubleDot]dinger problems", "Subsection",
 CellChangeTimes->{{3.634969471984861*^9, 3.634969486669629*^9}, {
   3.636357690741352*^9, 3.636357691621396*^9}, {3.6367814534149446`*^9, 
   3.636781462963572*^9}, {3.6367814980468884`*^9, 3.63678150276448*^9}, 
   3.636951119010036*^9, {3.636960420832326*^9, 3.636960439993699*^9}, {
   3.641034803447235*^9, 3.641034803783307*^9}, 3.656055984218356*^9, {
   3.673866132765826*^9, 3.67386614462851*^9}, {3.6933166117765007`*^9, 
   3.693316614401218*^9}, {3.694260573967292*^9, 3.69426057792155*^9}, 
   3.694260686732628*^9}],

Cell[TextData[{
 "Solution:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", "=", 
    RowBox[{
     RowBox[{"tr", " ", "log", " ", 
      FractionBox["O", 
       SubscriptBox["O", "reg"]]}], "=", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{
        SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
        RowBox[{
         FractionBox[
          RowBox[{"\[DifferentialD]", "p"}], "\[Pi]"], 
         RowBox[{"\[Delta]", "(", "p", ")"}]}]}]}], "+", 
      RowBox[{"divergent", " ", "term"}]}]}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.6933166433526793`*^9, 3.693316647838221*^9}, {
  3.6933191930697517`*^9, 3.6933191993311*^9}, {3.694260771292485*^9, 
  3.694260773004472*^9}, {3.694261050691436*^9, 3.694261052834235*^9}}],

Cell[CellGroupData[{

Cell["Setting", "Subsubsection",
 CellChangeTimes->{{3.636779905798805*^9, 3.636779914654092*^9}, {
   3.636781525735718*^9, 3.6367815291113987`*^9}, {3.636960461610382*^9, 
   3.6369604631073637`*^9}, {3.6410348083987007`*^9, 3.641034808732481*^9}, 
   3.69331664591294*^9}],

Cell[TextData[{
 "Consider ",
 Cell[BoxData[
  FormBox[
   RowBox[{"O", "=", 
    RowBox[{
     SuperscriptBox["\[Omega]", "2"], "+", "H"}]}], TraditionalForm]]],
 ", where\n\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"H", "[", 
      RowBox[{"x", ",", 
       RowBox[{
        SubscriptBox["\[PartialD]", "x"], 
        RowBox[{",", 
         SuperscriptBox[
          SubscriptBox["\[PartialD]", "x"], "2"]}]}]}], "]"}], 
     RowBox[{"\[CapitalPsi]", "(", "x", ")"}]}], "=", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["p", "2"]}], 
     RowBox[{"\[CapitalPsi]", "(", "x", ")"}]}]}], TraditionalForm]]],
 " \nsuch that at x>>1, it reduces to a plane wave equation\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["\[PartialD]", "2"], 
     RowBox[{"\[CapitalPsi]", "(", "x", ")"}]}], "=", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox[
       SubscriptBox["p", "0"], "2"]}], 
     RowBox[{"\[CapitalPsi]", "(", "x", ")"}]}]}], TraditionalForm]]],
 "\t\nThen, we can parametrize the wave function as\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[CapitalPsi]", "(", "x", ")"}], "=", 
     RowBox[{
      RowBox[{"A", "(", "p", ")"}], 
      RowBox[{"sin", "(", 
       RowBox[{
        RowBox[{"p", " ", "x"}], "+", 
        RowBox[{"\[Delta]", "(", "p", ")"}]}], ")"}]}]}], ",", " ", 
    RowBox[{"x", ">>", "1"}]}], TraditionalForm]]],
 "\n\tA, \[Delta] can be complex"
}], "Text",
 CellChangeTimes->{{3.634969495228086*^9, 3.634969513606598*^9}, {
   3.6349695572009573`*^9, 3.634969641472155*^9}, {3.63496971265337*^9, 
