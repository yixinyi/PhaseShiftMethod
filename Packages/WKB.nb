Cell[BoxData[{
 RowBox[{"BeginPackage", "[", "\"\<WKB`\>\"", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"WKBsolutions", "::", "usage"}], "=", 
    "\"\<WKBsolutions[order][do, params][x], where order determines the WKB \
expansion Exp[\[ImaginaryI] \!\(\*FractionBox[\(1\), \
\(e\)]\)(\!\(\*SubscriptBox[\(S\), \(0\)]\)(x)+ e \!\(\*SubscriptBox[\(S\), \
\(1\)]\)(x)+...+ \!\(\*SuperscriptBox[\(e\), \(order\)]\) \!\(\*SubscriptBox[\
\(S\), \(order\)]\)(x))],\nand do is the differential operator, x is the \
variable. \nThe output will be the solutions for the 1st derivatives of \
\!\(\*SubscriptBox[\(S\), \(i\)]\)(x). \nNote that PowerExpand is used to \
improve the speed!\n\>\""}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{"Begin", "[", "\"\<`Private`\>\"", "]"}], "\n", 
 RowBox[{"Needs", "[", "\"\<commons`\>\"", "]"}]}], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.6684325537193937`*^9, 3.668432555334464*^9}, {
   3.6684325945336447`*^9, 3.668432604406439*^9}, {3.668432671759685*^9, 
   3.6684326742360992`*^9}, {3.6684327119078617`*^9, 3.668432899369338*^9}, {
   3.668433046313558*^9, 3.6684330562561607`*^9}, 3.668433138304644*^9, {
   3.668433374238624*^9, 3.668433454052693*^9}, {3.6684335232552977`*^9, 
   3.668433523983042*^9}, {3.668437483627701*^9, 3.668437484332337*^9}, {
   3.6833555394466457`*^9, 3.6833555424728613`*^9}, {3.6927171585091057`*^9, 
   3.692717181469102*^9}, {3.692719458399953*^9, 3.6927194848899403`*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"WKBsolutions", "[", "order_Integer", "]"}], "[", 
     RowBox[{"do_", ",", "params_List"}], "]"}], "[", 
    RowBox[{"x_", "?", "notNumericQ"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "e", ",", "ansatz", ",", "listS", ",", " ", "listDerivS", ",", "eq", 
       ",", "componentsEq", ",", "series", ",", "solutionDerivS"}], "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"WKB", " ", "ansatz", " ", "and", " ", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"S0", "[", "x", "]"}], ",", " ", 
         RowBox[{"S1", "[", "x", "]"}], ",", " ", "..."}], "}"}]}], " ", 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{"ansatz", ",", "listS"}], "}"}], "=", 
       RowBox[{"WKBansatz", "[", 
        RowBox[{"order", ",", "e", ",", "x"}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"S0", "'"}], "[", "x", "]"}], ",", " ", 
         RowBox[{
          RowBox[{"S1", "'"}], "[", "x", "]"}], ",", " ", "..."}], "}"}], " ",
        "*)"}], "\[IndentingNewLine]", 
      RowBox[{"listDerivS", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"D", "[", 
          RowBox[{"#", ",", "x"}], "]"}], "&"}], "/@", "listS"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{
        "plug", " ", "in", " ", "the", " ", "ansatz", " ", "to", " ", "the", 
         " ", "differential", " ", "equation", " ", "with", " ", "the", " ", 
         "eigenvalue", " ", "part"}], ",", " ", 
        RowBox[{
        "the", " ", "final", " ", "equation", " ", "will", " ", "be", " ", 
         "equated", " ", "to", " ", "0"}]}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"eq", "=", 
       RowBox[{
        FractionBox[
         RowBox[{"do", "@", "ansatz"}], "ansatz"], "//", "Simplify"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{
        "expand", " ", "the", " ", "differential", " ", "equation", " ", 
         "for", " ", "large", " ", "p"}], ",", " ", 
        RowBox[{"we", " ", "subtract", " ", "2", " ", "because", " ", 
         RowBox[{"it", "'"}], "s", " ", "a", " ", "2", "nd", " ", "order", 
         " ", "DE"}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"series", "=", 
       RowBox[{"Series", "[", 
        RowBox[{
         RowBox[{"eq", "/.", 
          RowBox[{"listsToRule", "[", 
           RowBox[{"params", ",", 
            RowBox[{"params", "/", "e"}]}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"e", ",", "0", ",", 
           RowBox[{"order", "-", "2"}]}], "}"}]}], "]"}]}], ";", " ", 
      "\[IndentingNewLine]", 
      RowBox[{"componentsEq", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"List", "@@", 
           RowBox[{"LogicalExpand", "[", 
            RowBox[{"series", "\[Equal]", "0"}], "]"}]}], ")"}], "//", 
         "PowerExpand"}], "//", "Simplify"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{
        "solve", " ", "for", " ", "the", " ", "derivatives", " ", "of", " ", 
         SubscriptBox["S", "i"], " ", "in", " ", "terms", " ", "of", " ", 
         SubscriptBox["S", "j"], " ", "with", " ", "j"}], "<", "i"}], " ", 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"solutionDerivS", "=", 
       RowBox[{"Collect", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Solve", "[", 
            RowBox[{"componentsEq", ",", "listDerivS"}], "]"}], "//", 
           "Simplify"}], "//", "PowerExpand"}], ",", "params", ",", 
         "Simplify"}], "]"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"replace", " ", "2", "nd", " ", "derivatives", " ", "of", " ", 
         SubscriptBox["S", "i"]}], ",", " ", 
        RowBox[{
        "note", " ", "there", " ", "are", " ", "2", " ", "sets", " ", "of", 
         " ", "solutions"}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"replace2ndDeriv", "[", 
         RowBox[{"#", ",", "params", ",", "x"}], "]"}], "&"}], "/@", 
       "solutionDerivS"}]}]}], "\[IndentingNewLine]", "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"WKBansatz", "[", 
    RowBox[{"order_Integer", ",", 
     RowBox[{"e_", "?", "notNumericQ"}], ",", 
     RowBox[{"x_", "?", "notNumericQ"}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"list", ",", " ", "arg", ",", "ansatz"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"S0", "[", "x", "]"}], ",", " ", 
        RowBox[{"S1", "[", "x", "]"}], ",", " ", "..."}], "}"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"list", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"ToExpression", "[", 
           RowBox[{"\"\<S\>\"", "<>", 
            RowBox[{"ToString", "@", "i"}]}], "]"}], "[", "x", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "0", ",", "order"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "argument", " ", "expanded", " ", "in", " ", "small", " ", "e"}], " ", 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"arg", "=", 
       RowBox[{"list", ".", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"Power", "[", 
           RowBox[{"e", ",", "i"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", 
            RowBox[{"-", "1"}], ",", 
            RowBox[{"order", "-", "1"}]}], "}"}]}], "]"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"WKB", " ", "ansatz"}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"ansatz", "=", 
       RowBox[{"Exp", "[", 
        RowBox[{"\[ImaginaryI]", " ", "arg"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"ansatz", ",", "list"}], "}"}]}]}], "\[IndentingNewLine]", 
    "]"}]}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"replace2ndDeriv", "[", 
   RowBox[{"list_List", ",", "params_List", ",", 
    RowBox[{"x_", "?", "notNumericQ"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"solution", ",", "rule", ",", "ans", ",", "len", ",", "i"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "the", " ", "input", " ", "list", " ", "is", " ", "of", " ", "form", " ", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           SubscriptBox["S", "i"], "'"}], "[", "x", "]"}], " ", "\[Rule]", 
         " ", 
         RowBox[{
         "in", " ", "terms", " ", "of", " ", "derivatives", " ", "of", " ", 
          SubscriptBox["S", "j"]}]}], ",", " ", 
        RowBox[{"j", "<", "i"}], ",", " ", 
        RowBox[{"for", " ", "all", " ", "i"}]}], "}"}]}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "The", " ", "first", " ", "component", " ", "is", " ", "already", " ", 
      "good"}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"solution", "=", 
      RowBox[{"{", 
       RowBox[{"list", "[", 
        RowBox[{"[", "1", "]"}], "]"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Replace", " ", "the", " ", "2", "nd", " ", "derivatives", " ", "by", 
       " ", "the", " ", "derivatives", " ", "of", " ", "the", " ", "solution",
        " ", "list"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"len", "=", 
      RowBox[{"Length", "@", "list"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "2"}], ",", 
       RowBox[{"i", "\[LessEqual]", "len"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"rule", "=", 
         RowBox[{
          RowBox[{
           RowBox[{"D", "[", 
            RowBox[{"#", ",", "x"}], "]"}], "&"}], "/@", "solution"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"ans", "=", 
         RowBox[{
          RowBox[{"list", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "/.", "rule"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"solution", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"solution", ",", "ans"}], "]"}]}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Collect", "[", 
      RowBox[{"solution", ",", "params", ",", "Simplify"}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]}], "Input",
